{
	"name": "@typescript-boilerplate/example_pkg",
	"version": "0.0.0",
	"description": "A simple example sub package",
	"main": "index.ts",
	"prettier": "@typescript-boilerplate/config/prettier",
	"eslintConfig": {
		"extends": [
			"@typescript-boilerplate/config/eslint"
		]
	},
	"devDependencies": {},
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"format": "prettier --write \"**/*.{ts,tsx, js, jsx}\"",
		"format:check": "prettier --write \"**/*.{ts,tsx, js, jsx}\"",
		"docs": "typedoc --out docs src"
	},
	"keywords": [
		"sub-package",
		"typescript"
	],
	"author": "Aron Phillimore",
	"license": "MIT"
}
